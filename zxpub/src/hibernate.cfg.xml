<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory >
		<property name="connection.datasource">java:comp/env/jdbc/linping</property>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
		<mapping resource="com/zuxia/entity/ChildModule.hbm.xml" />
		<mapping resource="com/zuxia/entity/City.hbm.xml" />
		<mapping resource="com/zuxia/entity/FellowNote.hbm.xml" />
		<mapping resource="com/zuxia/entity/Module.hbm.xml" />
		<mapping resource="com/zuxia/entity/Note.hbm.xml" />
		<mapping resource="com/zuxia/entity/Password.hbm.xml" />
		<mapping resource="com/zuxia/entity/Province.hbm.xml" />
		<mapping resource="com/zuxia/entity/QuestionMaster.hbm.xml" />
		<mapping resource="com/zuxia/entity/SafeQuestion.hbm.xml" />
		<mapping resource="com/zuxia/entity/User.hbm.xml" />
	</session-factory>
</hibernate-configuration>
